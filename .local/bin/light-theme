dconf load /org/nemo/ < ~/.config/cinnamon/light/nemo.dconf
dconf load /org/gtk/ < ~/.config/cinnamon/light/gtk.dconf
dconf load /org/gnome/ < ~/.config/cinnamon/light/gnome.dconf

